      "id": "gamingServices",
      "title": "Disable Gaming Update",
      "category": "updater",
      "description": "Recreates firewall rules that block GamingServices and GamingServicesNet services from reaching the internet. Restore to remove the rules.",
      "localizedTitles": {
        "zh": "禁用 Gaming 更新"
      },
      "localizedDescriptions": {
        "zh": "重新创建阻止 GamingServices 及 GamingServicesNet 服务访问互联网的防火墙规则。恢复即可移除这些规则。"
      },
      "requiresAdmin": true,
      "riskLevel": "Low",
      "allowCancel": true,
      "serviceActions": [],
      "registryActions": [],
      "firewallActions": [
        {
          "ruleName": "BLOCK - GamingServices (service) - OUT",
          "direction": "out",
          "serviceName": "GamingServices",
          "group": "LWT Gaming Services"
        },
        {
          "ruleName": "BLOCK - GamingServices (service) - IN",
          "direction": "in",
          "serviceName": "GamingServices",
          "group": "LWT Gaming Services"
        },
        {
          "ruleName": "BLOCK - GamingServicesNet (service) - OUT",
          "direction": "out",
          "serviceName": "GamingServicesNet",
          "group": "LWT Gaming Services"
        },
        {
          "ruleName": "BLOCK - GamingServicesNet (service) - IN",
          "direction": "in",
          "serviceName": "GamingServicesNet",
          "group": "LWT Gaming Services"
        }
      ]
    },
